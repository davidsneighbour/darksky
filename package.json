{
  "name": "@davidsneighbour/hugo-darksky",
  "description": "A blog theme for GoHugo.",
  "version": "1.2024.14",
  "license": "MIT",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "keywords": [],
  "dependencies": {
    "@davidsneighbour/markdownlint-config": "2024.3.46",
    "@davidsneighbour/release-config": "2024.3.46",
    "@davidsneighbour/remark-config": "2024.3.46",
    "@davidsneighbour/tools": "2024.3.46",
    "wireit": "0.14.9",
    "pagefind": "1.1.1"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": ">=11.7.0"
  },
  "scripts": {
    "release": "wireit"
  },
  "wireit": {
    "release": {
      "command": "commit-and-tag-version --sign -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" -- --no-verify && ./bin/repo/release/postrelease"
    }
  },
  "main": "index.js"
}
