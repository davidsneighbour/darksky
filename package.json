{
  "name": "@davidsneighbour/hugo-darkskies",
  "description": "A blog theme for GoHugo.",
  "version": "1.2024.29",
  "license": "MIT",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "keywords": [],
  "dependencies": {
    "@davidsneighbour/markdownlint-config": "2024.4.8",
    "@davidsneighbour/release-config": "2024.4.8",
    "@davidsneighbour/remark-config": "2024.4.7",
    "@davidsneighbour/tools": "2024.4.7",
    "pagefind": "1.2.0",
    "sassdoc": "^2.7.4",
    "sassdoc-theme-herman": "^6.0.1",
    "sassdoc-extras": "^3.0.0"
  },
  "devDependencies": {
    "@types/youtube": "^0.1.0",
    "@types/node": "^22.8.6"
  },
  "scripts": {
    "release": "wireit",
    "sassdoc": "wireit"
  },
  "wireit": {
    "sassdoc": {
      "command": "sassdoc assets/scss --theme herman --dest static/documentation/sass",
      "files": [
        "assets/scss"
      ]
    },
    "sassdoc:debug": {
      "command": "sassdoc assets/scss --theme herman --dest static/documentation/sass --verbose --strict --debug",
      "files": [
        "assets/scss"
      ]
    },
    "release": {
      "command": "commit-and-tag-version --sign -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" -- --no-verify && ./bin/repo/release/postrelease"
    }
  },
  "main": "index.js"
}
